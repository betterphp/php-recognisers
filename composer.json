{
    "name": "betterphp/php-recogniser",
    "version": "0.1-dev",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/betterphp/php-code-style.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/betterphp/php-native-mock.git"
        }
    ],
    "require-dev": {
        "betterphp/php-native-mock": "dev-master",
        "betterphp/php-code-style": "dev-master",
        "squizlabs/php_codesniffer": "~2",
        "phpunit/phpunit": "~6"
    },
    "autoload": {
        "psr-4": {
            "betterphp\\recogniser\\": "./src"
        }
    }
}
